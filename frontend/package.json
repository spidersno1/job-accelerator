{
  "name": "job-accelerator-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:smoke-run": "cypress run --spec cypress/e2e/**/*.spec.ts",
    "test:smoke": "npm run dev & wait-on http://localhost:3000 --timeout 60000 && npm run test:smoke-run && taskkill /F /IM node.exe",
    "test:full": "concurrently \"cd ../backend && python -m uvicorn main:app --reload\" \"npm run dev\" && wait-on http://localhost:8000 --timeout 60000 && wait-on http://localhost:3000 --timeout 60000 && npm run test:smoke-run && taskkill /F /IM node.exe /IM python.exe",
    "dev": "powershell -Command \"if (Test-Path .next) { Remove-Item .next -Recurse -Force }\" && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "recharts": "^2.8.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.0.0",
    "jwt-decode": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wait-on": "^8.0.3",
    "@types/jwt-decode": "^3.1.0"
  }
}
